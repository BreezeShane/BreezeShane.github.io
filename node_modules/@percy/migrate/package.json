{
  "name": "@percy/migrate",
  "description": "Easily upgrade and migrate your Percy SDK to the latest version",
  "version": "0.10.0",
  "license": "MIT",
  "main": "dist/index.js",
  "bin": {
    "percy-migrate": "bin/run"
  },
  "files": [
    "bin",
    "dist",
    "transforms",
    "oclif.manifest.json"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "babel --root-mode upward src --out-dir dist",
    "clean": "rm -rf packages/**/{dist,.nyc_output,coverage,oclif.manifest.json}",
    "lint": "eslint --ignore-path .gitignore .",
    "postbuild": "oclif-dev manifest",
    "readme": "./scripts/readme",
    "test": "cross-env NODE_ENV=test mocha",
    "test:coverage": "nyc yarn test"
  },
  "publishConfig": {
    "access": "public"
  },
  "mocha": {
    "require": "@babel/register",
    "recursive": true
  },
  "oclif": {
    "bin": "npx @percy/migrate"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "@percy/config": "^1.0.0-beta.36",
    "@percy/logger": "^1.0.0-beta.36",
    "cross-spawn": "^7.0.3",
    "inquirer": "^8.0.0",
    "inquirer-glob-prompt": "^0.1.0",
    "jscodeshift": "^0.11.0",
    "semver": "^7.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.17",
    "@babel/core": "^7.12.17",
    "@babel/eslint-parser": "^7.12.17",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.12.17",
    "@babel/register": "^7.11.5",
    "@oclif/dev-cli": "^1.22.2",
    "babel-plugin-istanbul": "^6.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.20.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "expect": "^26.4.2",
    "mocha": "^8.3.0",
    "mock-require": "^3.0.3",
    "nyc": "^15.1.0"
  }
}
